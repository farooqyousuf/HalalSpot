<%= simple_form_for(@place) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :website %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Enter" ,class: "btn btn-primary" %>
  </div>
<% end %>
